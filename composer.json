{
  "name": "techies-africa/devpilot_laravel",
  "description": "Official laravel client for Devpilot",
  "keywords": [
    "ssh",
    "laravel",
    "devpilot",
    "deployments",
    "ci cd",
    "autodeploy",
    "deploy",
    "techies africa"
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "TechiesAfrica\\Devpilot\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Confidence Ugolo",
      "email": "ugoloconfidence@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2|^8.0",
    "guzzlehttp/guzzle": "^6.0|^7.0",
    "illuminate/support": "^v6.0|^v7.0|^v8.0|^v9.0",
    "symfony/console": "^v4.3|^v5.0|^v6.0",
    "symfony/process": "^v4.3|^v5.0|^v6.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TechiesAfrica\\Devpilot\\Providers\\DevpilotServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
