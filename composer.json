{
    "name": "techies-africa/devpilot_laravel",
    "description": "Official laravel client for Devpilot",
    "keywords": ["ssh", "ssh client", "server console", "terminal",  "laravel" , "devpilot" , "deployments" , "ci cd" , "autodeploy" , "deploy" , "techies africa"],
    "license": "MIT",
    "authors": [
        {
            "name": "Confidence Ugolo",
            "email": "ugoloconfidence@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "symfony/console": "^4.3|^5.0|^6.0",
        "symfony/process": "^4.3|^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^0.9.4|^1.3.1",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "TechiesAfrica\\Devpilot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TechiesAfrica\\Devpilot\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TechiesAfrica\\Devpilot\\Providers\\DevpilotServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
